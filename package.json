{
  "name": "@agewallet/js-sdk",
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/agewallet.umd.cjs",
  "module": "./dist/agewallet.js",
  "types": "./dist/agewallet.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agewallet-client/agewallet-js-sdk.git"
  },
  "exports": {
    ".": {
      "import": "./dist/agewallet.js",
      "require": "./dist/agewallet.umd.cjs"
    }
  },
  "files": [
    "dist/agewallet.js",
    "dist/agewallet.js.map",
    "dist/agewallet.umd.cjs",
    "dist/agewallet.umd.cjs.map",
    "dist/agewallet.min.js",
    "dist/agewallet.min.js.map"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "vite examples/frontend",
    "build:demo": "vite build",
    "build:lib": "vite build --config vite.lib.config.js",
    "build": "npm run build:demo && npm run build:lib"
  },
  "devDependencies": {
    "@upstash/redis": "^1.28.0",
    "happy-dom": "^20.0.11",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
